    width: 100%;
    height: 100%;
    position: absolute;
    top:0;
    left:0;
    z-index: 10;
}
.blog-slider-nav{
    width: auto;
    height: 50px;
    display: flex;
    flex-direction: row;
    position: relative;
}
.arrow-left,
.arrow-right{
    width:auto;
    height: 50px;
    box-sizing: border-box;
    cursor: pointer;
    opacity: .7;
    transition: .3s all linear;
}
.arrow-left svg,
.arrow-right svg{
