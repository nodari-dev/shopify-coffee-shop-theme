/*==COLLECTION==*/
section.collection-list.desktop{
    display: flex;
}
.collection-object{
    width:320px;
    height: 320px;
    margin-top: 20px;
    position: relative;
    background-size:cover;
    background-position: center center;
    transition: .2s all linear;
}
.collection-object.homepage{
    width: 28.3%;
    height: 80vh;
    box-sizing: border-box;
    min-height: 500px;
}
.collection-object.homepage:nth-child(4){
    width: 15%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: white;
    box-sizing: border-box;
    transition: .2 all linear;
}
.collection-object.homepage:nth-child(4) h2{
    writing-mode: vertical-lr;
    text-orientation: mixed;
    color:#191919;
    opacity: .7;
    transition: .2 all linear !important;
}
.collection-object.homepage:nth-child(4) a{
    width: 100%;
    height: 100%;
    position: absolute;
    top:0;
    left:0;
    z-index: 10;
}
.collection-object.homepage:nth-child(4):hover h2{
    opacity: 1;
}
.collection-title{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
    transition: .1s linear all;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    z-index: 3;
    align-items: center;
}
.collection-object h3{
    font-weight: 600;
    color:white;
}
.collection-object:hover .collection-title{
    opacity: 0;
}


.collection-object:hover .dark-image-bg{
    opacity: 0;

}
.no-product{
    width: 400px;
    min-height: 300px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    text-align: center;
}
.no-product img{
    width:275px;
    margin-bottom: 20px;
}

.collection-block{
    width:32%;
    height: 450px;
    box-sizing: border-box;
    position: relative;
    margin:1%;
    transition: .1s all linear;
    background-color: #191919;
}
.collection-block:nth-of-type(3n+1){
    margin-left: 0;
    }
.collection-block:nth-of-type(3n){
    margin-right: 0;
}
.collection-block a{
    position: absolute;
    width: 100%;
    height: 100%;
    top:0;
    left:0;
    z-index: 999;
}
.collection-block:hover{
    transform: scale(.95);
}
.collection-block .left{
    width: 100%;
    height: 100%;
    background-position: center;
    background-size: cover;
    cursor: pointer;
    position: relative;
    transition: .3s all linear;
}
.collection-block .left h2{
    z-index: 999;
    color:white;
    transition: .3s all linear;
}
.collection-block .right{
    width:30%;
    height: 100%;
    display: none;
    position: relative;
    transition: .3s all linear;
    background-color: #191919;
}
.collection-block.active .left {
    width:70%;
}
.collection-block.active .right{
    display: flex;
}


.new-collection-product{
    width:40%;
    height: 600px;
    box-sizing: border-box;
    padding: 1%;
    border-radius: 2px;
    background-color: #F2F6F2;
    transition: .1s linear all;
}
.new-collection-product .top{
    width: 100%;
    height: 100px;
    border-radius: 2px;
}
.new-collection-product .bottom{
    width: 100%;
    height: 500px;
}
.new-mini-collection-product{
    width: 50%;
    height: 300px;
    border:1px solid red;
    box-sizing: border-box;
}
/*==COLLECTION==*/

/*==LAST-PRODUCTS==*/



.product-el{
    width:23.5%;
    margin:1%;
    min-height: 360px;
    transition: .1s linear all;
    display: flex;
    justify-content:center;
    align-items: center;
    position: relative;
    background-color: white;
    box-sizing: border-box;
    border-radius: 2px;
}
.product-el:nth-of-type(4n+1){
    margin-left: 0;
}
.product-el:nth-of-type(4n){
    margin-right: 0;
}
.product-el a{
    width: 100%;
    height: 100%;
    z-index: 11;
    display: block;
    position: absolute;
    left:0;
}
.product-el-image{
    width: 100%;
    height: 320px;
    display: flex;
    align-items: center;
    justify-content: center;

}
.product-el-image img{
    height: 70%;
    z-index: 9;
}

.product-el-text{
    width:100%;
    height: 100%;
    box-sizing: border-box;
    background-color: #191919;
    color:white;
    display: flex;
    align-items: center;
    justify-content:center;
    text-align: center;
    position: absolute;
    transition: .1s linear all;
    border-radius: 2px;
    opacity: 0;
    z-index: 10;
}
.product-el:hover{
    transform: scale(.95);
}
.product-el:hover .product-el-text{
    opacity: .8;
}



/*==LAST-PRODUCTS==*/




/*==PRODUCTS==*/
.product-block{
    width: 23.5%;
    margin: 1%;
    padding: 1%;
    height: 500px;
    box-sizing: border-box;
    position: relative;
    display: flex;
    flex-direction: column;
    background-color: #F2F6F2;
    transition: .1s linear all;
    border: 4px solid #191919;
}
.product-block:nth-of-type(4n+1){
    margin-left: 0;
}
.product-block:nth-of-type(4n){
    margin-right: 0;
}
.collection-info-block{
    background-color: #191919;
    color:white;
    margin-left: 0;
    background-position: center center;
    background-size: cover;
}
.collection-info-block .top,
.product-block .top{
    width: 100%;
    height: 20%;
    position: relative;
    box-sizing: border-box;
}
.product-block .top .left{
    width:60%;
    height: 100%;
    box-sizing: border-box;
}
.collection-info-block .top h5{
    margin-top: -10px;
}
.product-block .top .left h5:first-child{
    font-weight: 600;
    margin-bottom: 5px;
}
.product-block .top .right{
    width:40%;
    height: 100%;
    box-sizing: border-box;
}
.product-block .top .right .add-cart,
.product-block .top .right .add-wish{
    width: 50%;
    height: 50px;
    box-sizing: border-box;
    opacity: .7;
    transition: .3 linear all;
}
.product-block .top .right .add-cart:hover,
.product-block .top .right .add-wish:hover{
    opacity: 1;
}
.product-block .bottom{
    width:100%;
    height: 80%;
    position: relative;
}
.product-block .bottom a{
    width: 100%;
    height: 100%;
    position: absolute;
    top:0;
    left:0;
}
.product-block img{
    width: 100%;
    height: auto;
}
.product-block form .shopify-payment-button{
    display: none;
}
.product-block form{
    width:100%;
    min-height: 30px;
    position: relative;
