/*===FOOTER===*/
footer{
    box-sizing: border-box;
    clear: both;
    width: 100%;
}
footer .wrapper{
    width:100%;
    min-height: 400px;
    box-sizing: border-box;
}
footer .wrapper.map{
    height: 400px;
}
footer .wrapper.info{
    padding-top: 100px;
}
footer .wrapper:last-child{
    width: 100%;
    min-height: 30px;
    padding: 20px;
}
footer iframe{
    width:100%;
    height: 100%;
}
ul.footer-social{
    display: flex;
    flex-direction: row;
}
ul.footer-social li a svg{
    width: 30px;
    height: 30px;
}
ul.footer-social li:first-child{
    margin-left:0;
}
ul.footer-social li{
    margin: 5px;
}
