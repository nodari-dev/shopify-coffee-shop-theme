*{
    margin: 0;
    padding: 0;
}
*::-moz-selection {
    background: #dc851f;
    color:white;
}
*::selection {
    background: #dc851f;
    color:white;
}
/*==TYPOGRAPHY==*/
@import url('https://fonts.googleapis.com/css2?family=Rubik:wght@400;600;800&display=swap');

html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, code,
del, dfn, em, img, q, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td {
  margin: 0;
  padding: 0;
  border: 0;
  font-weight: inherit;
  font-style: inherit;
  font-size: 100%;
  font-family: inherit;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}
section{
    width: 100%;
}
.shopify-section{
    width: 100%;
}
ul{
    list-style: none;
}
body{
    font-family: 'Rubik', sans-serif;
    color:white;
    width: 100%;
    height: 100%;
    padding:0;
    margin: 0;
    font-size: 16px;
    scroll-snap-type: y mandatory;
    background-color: #191919;
}
body#collections,
body#products{
    background-color: white;
    color:#191919;
}
em {
	font-style: italic;
}

strong {
	font-weight: bold;
}

p, select, option{
    line-height: 150%;
    font-size: 16px;
	font-weight: 400;
}
select{
    box-sizing: border-box;
    padding-left:10px;
    -moz-appearance:none;
    -webkit-appearance:none;
    appearance:none;
}
h1, 
h2, 
h3, 
h4, 
h5, 
h6 {
    text-transform: uppercase;
    word-break:break-word;
    font-weight: 800;
}
.big{
    font-size: 70px;
    display: inline-block;
}
h1 {
	font-size: 60px;
}
h2 {
	font-size: 34px;
}
h3 {
    font-size: 26px;
    font-weight: 600;
}
h4 {
    font-size: 22px;
    font-weight: 600;
}
h5{
    font-size: 20px;
    font-weight: 600;
}
h6{
    font-size: 18px;
    font-weight: 600;
}
.normal{
    font-weight: normal;
}
.headign-border-bottom{
    width: 100%;
    padding-bottom: 10px;
    border-bottom: 2px solid white;
}

.underline{
    text-decoration: underline;
}
a img {
	border: none;
	outline: none;
}

a {
	outline: none;
    text-decoration: none;
    font-size: 16px;
    font-weight: 400;
    color:white;
    transition: .1s all linear;
}

a,
a:active,
a:visited,
a img {
	outline: none;
}
.active-link{
    opacity: 1 !important;
}


button{
        min-width: 100px;
        min-height: 40px;
        font-size: 16px;
        border:none;
        outline: none;
        cursor: pointer;
        -webkit-appearance: none;
        transition: .1s all linear;
}
input{
    width: 245px;
    min-height: 40px;
    border:none;
    outline: none;
    box-sizing: border-box;
    padding:0 10px;
    font-size: 16px;
    transition: .1s all linear;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}
textarea{
    border:none;
    outline: none;
    -webkit-appearance: none;
    box-sizing: border-box;
    padding:10px;
    font-size: 16px;
    transition: .1s all linear;
    font-family: 'Rubik', sans-serif;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
}

.text-center{
    text-align: center;
}


/*===COLORS===*/
.white-bg{
    background-color: white;
    color:#191919;
}
.dark-bg{
    background-color: #191919;
    color:white;
}
/*===COLORS===*/


/*===BORDER-RADIUS===*/

.radius-left{
    border-radius: 2px 0px 0px 2px;
    -moz-border-radius: 2px 0px 0px 2px;
    -webkit-border-radius: 2px 0px 0px 2px;
}
.radius-right{
    border-radius: 0px 2px 2px 0px;
    -moz-border-radius: 0px 2px 2px 0px;
    -webkit-border-radius: 0px 2px 2px 0px;
}
.radius-all{
    border-radius: 2px !important;
}
.radius-none{
    border-radius: 0 !important;
}
/*===BORDER-RADIUS===*/


/*===ICONS===*/
svg.icon{
    fill:#191919;
}
svg.icon.white-icon{
    filter: invert(100%) sepia(0%) saturate(0%) hue-rotate(23deg) brightness(113%) contrast(101%);
}
svg.icon.dark-icon{
    filter: invert(100%) sepia(0%) saturate(0%) hue-rotate(23deg) brightness(113%) contrast(101%);
}

.icon-normal{
    width: 30px;
}
.icon-big{
    width: 45px;
}

.arrow-left-icon{
    transform: rotate(180deg);
}
/*===ICONS===*/



/*===BUTTONS===*/
button.dark-style,
button.light-style{
    border-radius:2px;
    cursor: pointer;
    font-weight: 600;
    z-index: 999;
    transition: .1s all linear;
    color:white;
    text-transform: uppercase;
    border-top-left-radius: 0px;
    border-bottom-left-radius: 0px;
}
button.light-style{
    background-color: #f6cb4b;
}
button.dark-style{
    background-color: #191919;
}

button.dark-style:hover,
button.light-style:hover{
    background-color: #dc851f;
}
button.menu-open{
    width: 100%;
    height: 40px;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}
/*===BUTTONS===*/

/*===INPUTS===*/

input.dark-style{
    border:2px solid #191919;
}
input.light-style{
    border:2px solid #f6cb4b;
}


input.button-light-style,
input.button-dark-style{
    cursor: pointer;
    font-weight: 600;
    text-transform: uppercase;
}
input.button-light-style{
    background-color: #f6cb4b;
    border:2px solid #f6cb4b;
    color:white;
}
input.button-dark-style{
    background-color: #191919;
    border:2px solid #191919;
    color:white;
}

input.button-light-style:hover,
input.button-dark-style:hover{
    background-color: #dc851f;
    border:2px solid #dc851f;
}
/*===INPUTS===*/





/*===LINKS===*/
.link-light, .link-dark{
    width: 200px;
    height: 40px;
    display: flex;
    align-items: center;
    justify-content: center;
    text-transform: uppercase;
    font-weight: 600;
    margin-top: 15px;
    border-radius: 2px;
    transition: .1s all linear;
}

.link-light{
    background-color: white;
    color:#191919;
}
.link-dark{
    background-color: #191919;
    color:white;
}
.link-light:hover{
    background-color: #f6cb4b;
}

.link-dark:hover{
    background-color: #dc851f;
}

a.light-style{
    color:white;
    transition: .1s all linear;
}
a.light-style h3,
a.dark-style h3{
    transition: .1s all linear;
}
a.light-style:hover{
    color: #f6cb4b;
}
a.light-style:hover h3{
    color: #dc851f;
}


/*===LINKS===*/



/*==CONTAINERS==*/
.desktop{
    display: block;
}
.mobile{
    display: none !important;
}
.no-scroll{
    height: 100%;
    overflow: hidden;
}
.row{
    display: flex;
    flex-direction: row !important;
    flex-wrap: wrap;
}
.row-reverse{
    display: flex;
    flex-direction: row-reverse !important;
    flex-wrap: wrap;
}
.row.between{
    display: flex;
    justify-content: space-between;
}
.row.center{
    display: flex;
    justify-content: center;
}
.row.start{
    display: flex;
    justify-content: flex-start;
}
.vertical-center{
    display: flex;
    align-items: center;
}
.vertical-start{
    display: flex;
    align-items: flex-start;
}
.horizontal-center{
    display: flex;
    justify-content: center;
}
.horizontal-right{
    display: flex;
    justify-content: right;
}
.column{
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
}
.column.center{
    align-items: center;
}
.column.start{
    justify-content: flex-start;
}
.center-all{
    display: flex;
    justify-content: center;
    align-items: center;
}
.shopify-section{
    scroll-snap-align: start;
}
.container-full{
    width: 100%;
    height: auto;
    display: flex;
    flex-wrap: wrap;
    position: relative;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
    justify-content: center;
    background-position: center center;
    background-size: cover;
}
.container-full.product{
    min-height: 600px;
}

.container{
    width:100%;
    padding: 0 6vw;
    height: auto;
    box-sizing: border-box;
    position: relative;
    z-index: 10;
}
.container.product{
    flex-direction: row;
}
/*===Margins===*/
.margin_45{
    margin: 45px 0;
}
.margin_top_45{
    margin-top: 45px;
}
.margin_bottom_45{
    margin-bottom: 45px;
}

.margin_30{
    margin:30px 0;
}
.margin_top_30{
    margin-top: 30px;
}
.margin_bottom_30{
    margin-bottom: 30px;
}

.margin_15{
    margin: 15px;
}
.margin_top_15{
    margin-top: 15px;
}
.margin_right_15{
    margin-right: 15px;
}
/*===Margins===*/

.padding_45px{
    padding: 45px 0;
}
.padding_bottom_45px{
    padding-bottom: 45px;
}
.padding_6vw{
    padding: 6vw;
}
.padding_2vw{
    padding: 2vw;
}
.container-full.homepage-video,
.container-full.full-height-nav{
    height: -moz-calc(100vh - 100px);
    height: -webkit-calc(100vh - 100px);
    height: -o-calc(100vh - 100px);
    height: calc(100vh - 100px);
    min-height: 600px;
}
.full-width{
    width: 100% !important;
    display: block;
}
.full-height{
    height: 100vh;
    min-height: 600px;
    background-position: center center;
    background-size: cover;
}
.container-full.big-image{
    background-size: cover;
    height: 500px;
}
.height_300{
    background-position: center center;
    background-size: cover;
    min-height: 300px;
}
.container-full.homepage-video .container{ 
    height: 100%;
}


.dark-image-bg{
    width: 100%;
    height: 100%;
    position: absolute;
    top:0;
    left:0;
    bottom: 0;
    right: 0;
    margin: auto;
    z-index: 2;
    background-color: #191919;
    opacity: .7;
    transition: .2s linear all;
}
/*==CONTAINERS==*/


/*==HEADER_NAV==*/
#header .container:first-child{
    border-bottom:2px solid #191919;
}
header.header{
    width:100%;
    min-height: 100px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: wrap;
    color:#191919;
    z-index: 999;
}

header.mini{
    position: fixed;
    top:0;
    left:0;
    min-width:80px;
    height: 80px;
    display: none;
    z-index: 999;
    background-color: white;
    color:#191919;
    cursor: pointer;
    line-height: 80px;
    background-position: center;
    background-size: cover;
    border-bottom-right-radius: 2px;
}
.work{
    display: flex !important;
}
header.mini h2{
    margin: 0 15px;
    vertical-align: middle;
}

.mini-content{
    width:200px;
    height: 80px;
    box-sizing: border-box;
    display: none;
    flex-direction: row;
}
.mini-content a{
    width:33.3%;
    height: 100%;
    box-sizing: border-box;
    text-transform: uppercase;
    color:#191919;
    font-weight: 600;
    position: relative;
}
.mini-content a:last-child{
    height: 50px;
}
.logo-with-menu h2{
    text-transform: uppercase;
}
.logo-with-menu a{
    color:#191919;
}
.logo-with-menu img{
    width:200px;
}


nav.navigation{
    width:auto;
    min-height: 100%;
    box-sizing: border-box;
    margin-left: 35px;
}
ul.menu{
    list-style: none;
}
ul.menu li:first-child{
    margin-left: 0;
}
ul.menu li{
    margin:0 10px;
}
ul.menu li.has-children ul{
    display: none;
}
ul.menu li a{
    opacity: 0.7;
    color:#191919;
    text-transform: uppercase;

}
ul.menu li a:hover{
    opacity: 1;
}


ul.tag-list{
    text-transform: uppercase;
}
ul.tag-list li{
    margin: 0 5px;
}
ul.tag-list li:first-child{
    margin: 0;
}

.mobile-menu{
    width:100%;
    min-height: 400px;
    height: 100vh;
    background-color: #191919;
    position: fixed;
    justify-content: center;
    align-items: center;
    top:0;
    left:0;
    z-index: 999;
    display: flex;
}
.mobile-menu ul.menu{
    flex-direction: column;
}
.mobile-menu ul.menu li{
    margin: 5px 0;
}
.mobile-menu ul.menu li a{
    color:white;
    font-size: 18px;
}
.disable-menu{
    display: none !important;
}
.header-actions {
    width: auto;
    height: auto;
    display: flex;
    flex-direction: row;
    align-items: center;
    box-sizing: border-box;
}
.header-actions .cart{
    width: 70px;
    height: 50px;
    background-color: white;
    box-sizing: border-box;
    position: relative;
}
.cart span{
    width: 25px;
    height: 25px;
    border-radius: 50%;
    background-color: #dc851f;
    position: absolute;
    top:0;
    right:0;
    color:white;

}
form.header-form{
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 50px;
}
.not-active{
    display: none;
}
header.header input{
    width: 340px;
    height: 100%;
}
header.header button{
    min-width: 70px;
    height: 100%;
    background: none;
}
.header-left h1.big{
    font-size: 100px;
}
.header-left,
.header-right{
    width: 50%;
    height: 100%;
    background-position: center center;
    background-size: cover;

    box-sizing: border-box;
}
.header-left{
    padding-left: 6vw;
}
.header-right{
    padding-right: 6vw;
}
