section.collection-list.desktop {
    display: flex;
}

.collection-object {
    min-height: 550px;
    height: 65vh;
    overflow: hidden;
    position: relative;
    @include default-transition;
    
    >a{
        width: 100%;
        height: 100%;
        @include absolute-position;
        z-index: 5;
    }
    img{
        @include object-image;
    }
    .collection-title {
        width: 100%;
        height: 100%;
        @include absolute-position;
        @include default-transition;
        z-index: 3;
        box-sizing: border-box;
        h3 {
            font-weight: 600;
