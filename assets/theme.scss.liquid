.product-description-block{
    .text-inner{
        div:not(.disable-read-mode){
            width: 70%;
        }
    }
}
.product-description-block{
    .text-inner{
        p,div{
            img{
                width: 60%;
            }
        }
    }
    
}
.disable-read-mode,
.disable-popup{
    width: 100px;
    height: 100px;
    position: absolute;
    top:0;
    right: 0;
    transition: .2s all linear;
    opacity: .6;
    cursor: pointer;
}
.disable-read-mode:hover,
.disable-popup:hover{
    opacity: 1;
}
.disable-read-mode span,
.disable-popup span{
    width: 80%;
    height: 4px;
    position: absolute;
    top:0;
    left:0;
    bottom:0;
    right:0;
    margin: auto;
    display: block;
    background-color: white;
}
.disable-popup{
    span{
        background-color: #191919;  
    }
}
.disable-read-mode span:nth-child(1),
.disable-popup span:nth-child(1){
    transform: rotate(45deg);
}
.disable-read-mode span:nth-child(2),
.disable-popup span:nth-child(2){
    transform: rotate(-45deg);
}
