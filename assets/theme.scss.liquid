.form-success {
    width: 100%;
    height: auto;
    text-align: center;
}

.shopify-challenge__container{
    min-height: -moz-calc(100vh - 100px);
    min-height: -webkit-calc(100vh - 100px);
    min-height: -o-calc(100vh - 100px);
    min-height: calc(100vh - 100px);
    margin-top: 100px;
    display: flex;
    flex-direction: column;
    justify-content:center;
    
    input{
        background-color: #191919;
        border: 2px solid #191919;
        color: white;
        cursor: pointer;
        text-transform: uppercase;
        font-weight: 600;
    }
    input:hover{
        background-color: #dc851f;
        border: 2px solid #dc851f;
    }
}
iframe.map{
    width: 100%;
    min-height: 65vh;
}

/*===CONTACT-PAGE===*/


/*===FOOTER===*/

footer {
    position: relative;
    box-sizing: border-box;
    clear: both;
    z-index: 10;
}
footer:before,
footer:after {
    content:"";
    display:table;
    line-height:0;
}
footer:after {
    clear:both;
}
.footer-wrap{
    ul.menu{
        margin: 10px 0;
        li{
            margin: 2px 5px;
        }
        >li{
            a{
                font-size: 1em;
                font-weight: 400;
                color:#ffffff;  
                @include default-transition;
                svg{
                    fill: #ffffff;
                }
            }
            >a{
                font-weight: 600;
            }
            a:hover{
                color:#f6cb4b;
            }
            a::after{
                display: none;
            }
            ul{
                font-size: 1em;
            }
            ul.menu-child,
            ul.menu-grandchild{
                margin: 0 10px;
            }
        }
    }
    ul.social-footer{
        width: 100%;
        li{
            a{
                svg{
                    fill: white !important;
                }
                span{
                    margin-left: 10px;
                }
            }
        }
    }
}

/*===FOOTER===*/

/*===MOBILE===*/

/* Extra small devices (phones, 600px and down) */
@media only screen and (max-width: 600px) {
    .big{
        font-size: 2.5em;
    }
    ul.partner-slider{
        li{
            img.partner {
                width: 100%;
            }
        }
    }
    h1 {
        font-size: 2.3em;
    }
    h2 {
        font-size: 1.9em;
    }

    h3 {
        font-size: 1.6em;
    }

    a.logo{
        font-size: 1.3em;
    }
    .image-with-text-wrap:nth-child(2){
        min-height: 50vh;
    }


    ul.partner-list{
        li{
            img.partner{
                width: 40%;
            }
        }
    }
    .container.blog{
        padding: 0 6vw;
    }
    .post-info{
        div{
            width: 100%;
        }
    }
    #comments{
        ul{
            li{
                width: 100%;
            }
        }
    }
    .comment-form{
        input{
            width: 100%;
        }
        textarea{
            width: 100%;
        }
        button{
            width: 100%;
        }
    }
    .product-block{
        .top{
            flex-direction: column;
            .left{
                width: 100%;
            }
            .right{
                width: 100%;
                height: auto;
            }
        }
    }
    .template-product{
        .product-detail-photo{
            width:100%;
        }
        .product-image-photo-wrap.desktop{
            display: none;
        }
        .product-image-photo-wrap.mobile,
        .container.mobile{
            display: block !important;
        }
        .container.reviews{
            width: 100%;
        }
        .product-detail-text{
            width: 100%;
            padding-left: 6vw;
            margin-top: 30px;
            .product-action{
                .select-product-list{
                    width: 100%;
                }
                input.product-page{
                    width: 100%;
                }
            }
        }
        .product-description-block{
            .text-inner{
                padding: 20vw 6vw 6vw 6vw !important;
            }
        }
    }
    .template-cart{
        .container{
            width: 100%;
        }
        .cart-result-wrap{
            position: relative !important;
            margin-top: 30px !important;
        }
        .cart-block{
            .product-image{
                width: 100%;
            }
            .product-info{
                width: 100%;
                .product-main{
                    width: 100%;
                    min-height: 15vh;
                }
                .product-count{
                    width: 100%;
                    min-height: 10vh;
                    input{
                        text-align: center;
                    }
                }
                .product-action{
                    width: 35%;
                    min-height: 10vh;
                }
            }
        }
    }
    .template-search{
        form.search{
            input,
            button{
                text-align: center;
                width: 100% !important;
            }
        }
    }
}

