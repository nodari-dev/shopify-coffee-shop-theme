header.header {
    width: 100%;
    min-height: 100px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: wrap;
    color: #191919;
    z-index: 999;
}
#header{
    z-index: 999;
    border-bottom: 2px solid #f2f2f2;
}
#header{
    h2, a{
        color:#191919;
        text-transform: uppercase;
    }
}
a.logo{
    font-size: 2em;
    font-weight: 800;
}
.header-nav{
    width: auto;
    height: 100%;
    box-sizing: border-box;
}
.open-menu{
    width: 50px;
    cursor: pointer;
    margin-left: 15px;
}
.open-menu{
    span{
        width: 30px;
        height: 3px;
        margin-top: 4px;
        display: block;
        background-color: #191919;
        transition: .2s all linear;
    }
    span:nth-child(1){
        margin-top: 0;
    }
    span:nth-child(2){
        margin-left: 5px;
    }
    span:nth-child(3){
        margin-left: 10px;
    }
}
.open-menu:hover{ 
    span{
        margin-left: 0;
    } 
}
.close-menu{
    cursor: pointer;
}
.header-menu{
    z-index: 999;
    display: flex;
    transition: .2s linear all;  
    opacity: 1;
    left:0;
    box-sizing: border-box;
    -ms-overflow-style: none;
    scrollbar-width: none;
}
.header-menu.hidden{
    left:-100%;
}
nav.navigation{
    height: 100vh;
    overflow-y: scroll;
    overflow-x: hidden;
    box-sizing: border-box;
    -ms-overflow-style: none;
    scrollbar-width: none;
}

nav.navigation::-webkit-scrollbar{
    display: none;
}
ul.menu {
    list-style: none;
}

ul.menu{
    li{
        margin: 5px 0;
    }
}
ul.menu{
    li{
        a{
            top: 3px;
            opacity: 0.7;
            font-weight: 600;
            color: #191919;
            text-transform: uppercase;
            position: relative;
        }
    }
}
ul.menu{
    li{
        a::after{
            width: 0;
            height: 6px;
            background-color: #191919;
            display: block;
            content: "";
            transition: .2s all linear;
        }
    }
}
ul.menu{
    li{
        a:hover{
            opacity: 1;
        }
    }
}
ul.menu{
    li{
        a:hover::after{
            width: 100%;
        }
    }
}
ul.menu{
    li{
        ul{
            margin-left: 30px;
        }
    }
}
ul.menu{
    li{
        ul{
            li{
                a.big{
                    font-size:2em;
                }
                a::after{
                    height: 3px;
                }
            }
        }
    }
}


ul.tag-list {
    text-transform: uppercase;
}

ul.tag-list li {
    margin: 0 5px;
}

ul.tag-list li:first-child {
