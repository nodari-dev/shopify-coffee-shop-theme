/*==TYPOGRAPHY==*/
@import url('https://fonts.googleapis.com/css2?family=Rubik:wght@400;600;800&display=swap');
@mixin background-image{
    position: absolute;
    max-width: none;
    max-height: none;
    display: block;
}

@mixin default-transition{
    transition: .2s all ease-out;
}
@mixin slow-transition{
    transition: .6s all ease-out;
}
@mixin object-image{
    width: 100%;
    height: 100%;
    object-fit: cover;
}

@mixin absolute-position{
    position: absolute;
    top:0;
    left:0;
    bottom: 0;
    right: 0;
    margin: auto;
}
@mixin absolute-position-right-top{
    position: absolute;
    top:0;
    right: 0;
    margin: auto;
}
