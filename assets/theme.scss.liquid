/*==HEADER_NAV==*/

header.header {
    width: 100%;
    min-height: 100px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    flex-wrap: wrap;
    color: #191919;
    z-index: 999;
}
#header{
    z-index: 999;
    border-bottom: 2px solid #f2f2f2;
}
#header{
    h2, a{
        color:#191919;
        text-transform: uppercase;
    }
}
a.logo{
    font-size: 2em;
    font-weight: 800;
}
.header-nav{
    width: auto;
    height: 100%;
    box-sizing: border-box;
}
.open-menu{
    width: 50px;
    cursor: pointer;
    margin-left: 15px;
}
.open-menu{
    span{
        width: 30px;
        height: 3px;
        margin-top: 4px;
        display: block;
        background-color: #191919;
        @include default-transition;
    }
    span:nth-child(1){
        margin-top: 0;
    }
    span:nth-child(2){
        margin-left: 5px;
    }
    span:nth-child(3){
        margin-left: 10px;
    }
}
.open-menu:hover{ 
    span{
        margin-left: 0;
    } 
}
.close-menu{
    cursor: pointer;
}
.header-menu{
    z-index: 999;
    display: flex;
    @include default-transition;
    opacity: 1;
    left:0;
    box-sizing: border-box;
    -ms-overflow-style: none;
    scrollbar-width: none;
    
    .container:nth-child(2){
        @include absolute-position;
        z-index: 2;
    }
}
.header-menu.hidden{
    left:-100%;
}
nav.navigation{
    height: 100vh;
    overflow-y: scroll;
    overflow-x: hidden;
    box-sizing: border-box;
    -ms-overflow-style: none;
    scrollbar-width: none;
    z-index: 800;
}

nav.navigation::-webkit-scrollbar{
    display: none;
}
ul.menu{
    list-style: none;
    li{
        margin: 5px 0;
        a{
            top: 3px;
            opacity: 0.7;
            font-weight: 600;
            color: white;
            text-transform: uppercase;
            position: relative;
        }
        >a{
            opacity: 1;
        }
        a::after{
            width: 0;
            height: 6px;
            background-color: #f6cb4b;
            display: block;
            content: "";
            @include default-transition;
        }
        a:hover{
            opacity: 1;
            color:#f6cb4b;
            &::after{
                width: 100%;
            }
        }
        ul{
            margin-left: 30px;
            li{
                a.big{
                    font-size:2em;
                }
                a::after{
                    height: 3px;
                }
            }
        }
    }
    
}

ul.taglist{
    margin-left: -10px;
    li{
        background-color: #191919;
        margin:10px;
        a{
            text-transform: uppercase;
        }
    }
}
.disable-menu {
    display: none !important;
}

.header-main{
    box-sizing: border-box;
    position: relative;

    .container{
        height: 100%;
        .main-hero{
            & a{
                min-width: 200px;
                height: 200px;
                border-radius: 50%;
                background-color: #191919;
                border:2px solid #191919;
                margin-left: 25px;
                font-size: 2.2em;
                @include default-transition;
            }
            & a:hover{
                background-color: white;
                color:#191919;
            }
        }
    }

}
.partners-wrapper{
    min-height: calc( 10vh);
}

ul.partner-slider{
    width:100%;
    height: 100%;
    li{
        display: flex !important;
        min-height: 10vh;
        opacity: .7;
        @include default-transition;
        box-sizing: border-box;
        img.partner {
            width: 45%;
        }
    }
    li:hover{
        opacity: 1;
    }
}


