.loader{
    width: 100%;
    height: 100vh;
    position: fixed;
    top:0;
    box-sizing: border-box;
    left:0;
    z-index: 9999;
    transition: .5s all linear;
}
.loader.hide{
    left:-100%;
}
.loader{
    h2{
        -webkit-animation: 1.5s linear 0s infinite alternate fade_cycle;
        -moz-animation: 1.5s linear 0s infinite alternate fade_cycle;
          -o-animation: 1.5s linear 0s infinite alternate fade_cycle;
             animation: 1.5s linear 0s infinite alternate fade_cycle;
    }
}

@-webkit-keyframes fade_cycle { from { opacity: .15; } to { opacity: 1; }  }
   @-moz-keyframes fade_cycle { from { opacity: .15; } to { opacity: 1; }  }
     @-o-keyframes fade_cycle { from { opacity: .15; } to { opacity: 1; }  }
        @keyframes fade_cycle { from { opacity: .15; } to { opacity: 1; }  }


.popup{
    width:40%;
    height: 40%;
    position: fixed;
    top:0;
    left:0;
    bottom: 0;
    right:0;
    margin: auto;
    align-items: center;
    justify-content: center;
    z-index: 9999;
    opacity: 0;
    display: none;
    transition: .2s all linear;

}
.popup.show,
.popup-dark.show{
    display: flex;
    transition: .2s all linear;
}
.popup.show{
    -webkit-animation: .2s linear 0s alternate popup_show;
    -moz-animation: .2s linear 0s alternate popup_show;
      -o-animation: .2s linear 0s alternate popup_show;
         animation: .2s linear 0s alternate popup_show;
         animation-fill-mode: forwards
}
@-webkit-keyframes popup_show { from { opacity: 0; } to { opacity: 1; }  }
   @-moz-keyframes popup_show { from { opacity: 0; } to { opacity: 1; }  }
     @-o-keyframes popup_show { from { opacity: 0; } to { opacity: 1; }  }
        @keyframes popup_show { from { opacity: 0; } to { opacity: 1; }  }

.popup-dark.show{
