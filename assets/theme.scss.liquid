/*===BLOG SECTION/PAGE===*/
.blog-slider{
    width:100%;
    height:350px;
    box-sizing: border-box;
}
.blog-slider .slick-list{
    margin: 0 -7.5px;
}
.blog-slider .blog-el{
    box-sizing: border-box;
    height: 350px;
    display: flex !important;
    position: relative;
    flex-direction: row;
    flex-wrap: wrap;
    transition: .1s linear all;
    margin:0 7.5px;
    border:4px solid #191919;
}
.blog-el:hover{
    transform: scale(.95);
    -webkit-box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.75);
    -moz-box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.75);
    box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.75);
}

.blog-slider .blog-el .left{
    width:100%;
    height: 100%;
    background-size: cover;
    background-position: center;
    position: absolute;
    top:0;
    left:0;
    z-index: 2;
}

.blog-slider .blog-el .right{
    width:100%;
    height: 100%;
    box-sizing: border-box;
    text-align: left;
    padding:3vw 2vw;
