/*===CART-PAGE===*/

.template-cart{
    background-color: white;
    color:#191919;

    form{
        width: 100%;
    }
}
.cart-block{
    width: 100%;
    position: relative;
    border-bottom: 2px solid #f2f2f2;
}
.cart-block:last-child{
    border:none;
}

.cart-block{
    .product-image{
        width:20%;
        min-height: 200px;
        background-position: center center;
        background-size: cover;
        position: relative;
        
        a{
            width: 100%;
            height: 100%;
            position: absolute;
            top:0;
            left:0;

        }
    }
}

.cart-block{
    .product-info{
        width:80%;
        min-height: 200px;
        box-sizing: border-box;

        .product-main,
        .product-count,
        .product-action{
            box-sizing: border-box;
            min-height: 20vh;
        }
        .product-main{
            width: 60%;
            a{
                h4{
                    color:#191919;
                }
            }
        }
        .product-count{
            width: 25%;
            input{
                width: 100%;
                background-color: #f2f2f2;
                color:#191919;
            }
        }
        .product-action{
            width: 15%;
            a{
                color:#191919;
            }
        }
    }
}
.container.checkout{
    .cart-result{
        width: 100%;
        padding: 0 6vw;
        position: absolute;
        box-sizing: border-box;
        top:0;
        left:0;

        .top{
            width: 100%;
            min-height:10vh;
        }
        .bottom{
            width: 100%;
            min-height: 10vh;
        }
    }
    input{
        width: 100%;
        cursor: pointer;
    }
}
