/*FUCKING GRID*/

.four-per-row, .four-per-row-no-margin,
.three-per-row, .three-per-row-no-margin,
.two-per-row, .two-per-row-no-margin,
.one-per-row, .one-per-row-no-margin{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.four-per-row .grid-el {
    width: 23.5%;
    margin: 1%;
}
.four-per-row-no-margin .grid-el{
    width: 25%;
}
.four-per-row .grid-el:nth-of-type(4n+1) {
    margin-left: 0;
}

.four-per-row .grid-el:nth-of-type(4n) {
    margin-right: 0;
}

.three-per-row .grid-el {
    width: 32%;
    margin: 1%;
}
.three-per-row-no-margin .grid-el{
    width: 33.3%;
}
.three-per-row .grid-el:nth-of-type(3n+1) {
    margin-left: 0;
}

.three-per-row .grid-el:nth-of-type(3n) {
    margin-right: 0;
}

.two-per-row .grid-el {
    width: 49%;
    margin: 1%;
}
.two-per-row-no-margin .grid-el{
    width: 50%;
}

.two-per-row .grid-el:nth-of-type(2n+1) {
    margin-left: 0;
}

.two-per-row .grid-el:nth-of-type(2n) {
    margin-right: 0;
}

.one-per-row .grid-el {
    width: 100%;
    margin:1% 0;
}
.one-per-row-no-margin .grid-el{
    width: 100%;
}

